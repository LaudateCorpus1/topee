{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "webextensions": true,
    "commonjs": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "safari": true
  },
  "rules": {
    "no-console": 0,
    "semi": ["error", "always"]
  }
}
